<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ClientAdmin</title>
    <script type="importmap"></script>
    <link rel="stylesheet" href="~/css/style.css" />
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link href="~/css/swiper-bundle.min.css" rel="stylesheet" />
</head>
<body>
  <div class="page">
       <div class="app-header">
            <partial name="_Navbar" />
       </div>
        <partial name="_SideBar" />
        <div class="main-content">
           @RenderBody()
            <footer class="footer">
                <div class="container">
                    <span class="text-muted">
                        Copyright © <span id="year"> 2025 </span>. Designed by
                        <a href="#" target="_blank"> <span> Fortune4 Technogies. </span> </a>
                        All rights reserved
                    </span>
                </div>
            </footer>
         </div>

        
  </div>
    @await RenderSectionAsync("Scripts", required: false)



    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script> 
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script> 
    <script src="~/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.min.js"></script> 
     <script src="~/js/popper.min.js" crossorigin="anonymous"></script>
    <script src="~/js/bootstrap.min.js"></script>
     <script src="~/js/moment.min.js"></script>
    <script src="~/lib/swiper-bundle.min.js"></script>
    <script src="~/js/daterangepicker.min.js"></script>
    <script src="~/js/commonHelper.js" asp-append-version="true"></script>
    <script src="~/js/ajaxhelper.js" asp-append-version="true"></script>
    <script src="~/js/unobtrusiveHelper.js" asp-append-version="true"></script>
    <script src="~/js/site.js"></script>
</body>
</html>
